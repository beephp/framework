{
    "name": "beephp/framework",
    "description": "A lightweight PHP micro framework for building web applications.",
    "keywords": ["framework", "php", "micro"],
    "license": "MIT",
    "homepage": "https://beephp.ir",
    "support": {
        "issues": "https://github.com/beephp/framework/issues",
        "source": "https://github.com/beephp/framework"
    },
    "authors": [
        {
            "name": "usermp",
            "email": "usermp76@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-session": "*",
        "ext-tokenizer": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-xml": "*",
        "guzzlehttp/guzzle": "^7.8.2",
        "guzzlehttp/uri-template": "^1.0",
        "monolog/monolog": "^3.0",
        "symfony/var-dumper": "^7.2.0",
        "symfony/console": "^7.2.0",
        "symfony/error-handler": "^7.2.0",
        "symfony/finder": "^7.2.0",
        "symfony/http-foundation": "^7.2.0",
        "symfony/http-kernel": "^7.2.0",
        "symfony/mailer": "^7.2.0",
        "symfony/mime": "^7.2.0",
        "symfony/polyfill-php83": "^1.31",
        "symfony/process": "^7.2.0",
        "symfony/routing": "^7.2.0",
        "symfony/uid": "^7.2.0",
        "vlucas/phpdotenv": "^5.6.1"
    },
    "require-dev": {
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Beephp\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Beephp\\Framework\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
    },
    "suggest": {
    }
}
